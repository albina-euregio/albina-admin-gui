{
  "i18n-ally.localesPaths": "src/assets/i18n",
  "i18n-ally.regex.usageMatchAppend": ["\\bt\\(['\"`]({key})['\"`]"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.keysInUse": [
    "avalancheProblem.*",
    "dangerRating.*",
    "aspect.*",
    "units.*",
    "LoLaAvalanchePotential.*",
    "observations.eventTypes.*",
    "LoLaWeather.*",
    "snowpack.plotType.*",
    "snowpack.aspect.*",
    "snowpack.meteo.*",
    "bulletins.table.title.status.*",
    "role.*"
  ],
  "nuxt.isNuxtApp": false,
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter",
    "editor.formatOnSave": true
  }
}
